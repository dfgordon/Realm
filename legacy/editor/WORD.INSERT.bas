 0  REM INSERT AN ASCII STRING IN A MAP FILE
 5  FOR A = 37664 TO 37664
 10  INPUT A$
 20  FOR I = 1 TO  LEN (A$)
 30 M =  ASC ( MID$ (A$,I,1))
 31  PRINT  MID$ (A$,I,1)
 40  POKE A + (I - 1),M
 50  NEXT
 60  POKE A + I - 1,0
 70  NEXT
